<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane fx:id="root" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox id="optionsContainer" fx:id="optionsContainer" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" stylesheets="@../../styles/FoldersContainerElementOptionsForm.css">
         <children>
            <Button fx:id="deleteButton" mnemonicParsing="false" style="-fx-font-size: 16px;" styleClass="hoverable" text="Delete" />
            <Button fx:id="copyButton" mnemonicParsing="false" style="-fx-font-size: 16px;" styleClass="hoverable" text="Copy" />
         </children>
         <effect>
            <DropShadow />
         </effect>
      </VBox>
   </children>
</AnchorPane>
