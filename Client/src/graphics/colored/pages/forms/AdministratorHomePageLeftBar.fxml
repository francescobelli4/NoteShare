<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox id="leftbar" prefHeight="200.0" prefWidth="100.0" stylesheets="@../styles/StudentHomePageForm.css" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphics2.colored.ScreenColoredAdministratorHomePageLeftBarController">
   <effect>
      <DropShadow />
   </effect>
   <children>
      <VBox VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="reported_notes_button" alignment="BASELINE_LEFT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Reported Notes">
               <graphic>
                  <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" smooth="false">
                     <image>
                        <Image url="@../styles/icons/error.png" />
                     </image>
                  </ImageView>
               </graphic>
               <font>
                  <Font name="Cantarell Regular" size="32.0" />
               </font>
               <styleClass>
                  <String fx:value="leftbar_button" />
                  <String fx:value="selected" />
               </styleClass>
            </Button>
         </children>
      </VBox>
      <VBox alignment="BOTTOM_CENTER" prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS">
         <children>
            <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../styles/icons/user_image.png" />
               </image>
            </ImageView>
            <Label fx:id="username_label" text="Username">
               <font>
                  <Font name="Cantarell Regular" size="42.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
</VBox>
