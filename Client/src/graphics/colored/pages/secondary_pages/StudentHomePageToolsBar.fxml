<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphics.colored.ScreenColoredStudentHomePageToolsBarController">
    <children>
        <Button mnemonicParsing="false" onAction="#onFolderAddButtonClick" style="-fx-background-color: white;">
            <font>
                <Font size="29.0" />
            </font>
            <graphic>
                <ImageView fitHeight="90.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../styles/icons/folder_plus.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button mnemonicParsing="false" onAction="#onFileAddButtonClick" style="-fx-background-color: white;">
            <font>
                <Font size="29.0" />
            </font>
            <graphic>
                <ImageView fitHeight="90.0" fitWidth="140.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../styles/icons/file_plus.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
</HBox>
